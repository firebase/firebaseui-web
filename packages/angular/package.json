  "typings": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/fesm2022/firebase-ui-angular.mjs"
    }
  },
  "scripts": {
    "prepare": "pnpm run build",
    "build": "ng-packagr -p ng-package.json",
